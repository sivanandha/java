package com.example;

interface WaterFun {
	String color = "blue";

	void fillup();
	void flowwater();
}

public class InterfaceFun implements WaterFun {

	public static void main(String[] args) {
		System.out.println(color);
		InterfaceFun i = new InterfaceFun();
		i.fillup();

	}

	@Override
	public void fillup() {
		System.out.println("water is fillup quickely");

	}

}

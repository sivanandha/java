package com.example.Arraylist;

import java.util.ArrayList;
import java.util.Iterator;

public class ArrayList1 {

	public static void main(String[] args) {

		/*
		 * ArrayList<String> list = new ArrayList<String>();// Creating arraylist
		 * list.add("Ravi");// Adding object in arraylist list.add("Vijay");
		 * list.add("Ravi"); list.add("Ajay"); // Traversing list through Iterator
		 * Iterator itr = list.iterator(); while (itr.hasNext()) {
		 * System.out.println(itr.next()); }
		 */

		ArrayList<String> arrayList = new ArrayList<>();
		arrayList.add("mallaiah");
		arrayList.add("sivanandha");
		arrayList.add("lucky");
		System.out.println(arrayList);
		Iterator<> itr = new Iterator<>();
	}

}
